    "def get_job_desc(search_url):\n",
    "    html = requests.get(search_url).content\n",
    "    soup = bs(html, \"html.parser\")\n",
    "    results = soup.find(id=\"resultsCol\")\n",
    "    \n",
    "    divs = results.find_all(\"div\")\n",
    "    for div in divs:\n",
    "        if div.get(\"id\") == \"searchCount\":\n",
    "            count = div\n",
    "    count = count.get_text()\n",
    "    count = count.split(\" \")[-4]\n",
    "    count = int(count.replace(\".\",\"\"))\n",
    "    \n",
    "    print(\"Getting job ids...\")\n",
    "    \n",
    "    job_ids = []\n",
    "    for i in range(0, count, 10):\n",
    "        url = search_url + \"&start=\" + str(i)\n",
    "        job_ids = job_ids + get_links(url)\n",
    "        sleep(.2)\n",
    "        \n",
    "    print(\"Finished pulling job ids\")\n",
    "    \n",
    "    descriptions = []\n",
    "    counter = 0\n",
    "    \n",
    "    for ids in job_ids:\n",
    "        if counter % 10 == 0:\n",
    "            print(\"Pulling job n° \" + str(counter))\n",
    "        counter +=1\n",
    "        url = \"https://it.indeed.com/viewjob?jk=\" + str(ids)\n",
    "        descriptions.append(get_text(url))\n",
    "        sleep(.2)\n",
    "        \n",
    "    return descriptions\n",
    "\n"
   ]
